<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.airjnc.admin.dao.AdminMapper">
  <update id="updateRoom">
    UPDATE room
    SET status=#{status}
    WHERE id = #{id}
  </update>
</mapper>
